function ai_get_params(e){for(var r=document.getElementsByTagName("script"),t=0;t<r.length;t++)if(r[t].src.indexOf("/"+e)>-1){for(var a=r[t].src.split("?").pop().split("&"),n={},o=0;o<a.length;o++){var c=a[o].split("=");n[c[0]]=c[1]}var p=r[t].src.split(e);return n.dmn=p[0],n}return{}}function ai_log(e,r,t,a){if(t)var n=t;else var n=document.location.href;var o=n.indexOf("//");n=o>=0?n.substr(o):"//"+document.location.host+("/"==n[0]?"":"/")+n;var c=ai_get_url_stat(e,r,n,a);"undefined"!=typeof force_popup&&force_popup&&(c+="&force_popup=1"),"undefined"!=typeof jQuery?c+="&js=jquery":"undefined"!=typeof Prototype&&(c+="&js=prototype");var p=document.createElement("script");p.type="text/javascript",p.async=!0,p.src=c;var i=document.getElementsByTagName("script")[0];i.parentNode.insertBefore(p,i)}function ai_get_url_stat(e,r,t,a){a&&(ref=a);var n=new Date,o=e,c=window.devicePixelRatio>1?"1":"0";return o+="?url="+escape(t),o+="&id="+r+"&res="+screen.width+"x"+screen.height+"&col="+screen.colorDepth,o+="&h="+n.getHours()+"&m="+n.getMinutes()+"&s="+n.getSeconds(),o+="&dy="+n.getFullYear()+"&dm="+n.getMonth()+"&dd="+n.getDate(),o+="&ref="+escape(ref),o+="&retina="+c}var pmv_agent=navigator.userAgent.toLowerCase(),pmv_cookie=navigator.cookieEnabled?"1":"0",ref="";try{ref=top.document.referrer}catch(e){if(parent){if(parent.get_ref)try{ref=parent.get_ref}catch(E3){ref=""}else try{ref=parent.document.referrer}catch(E){try{ref=document.referrer}catch(E2){ref=""}}parent.get_ref=document.location.href}else try{ref=document.referrer}catch(E3){ref=""}}var p=ai_get_params(".js");ai_log(p.dmn,p.i,p.u,p.r);